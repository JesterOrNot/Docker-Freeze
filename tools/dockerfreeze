_dockerfreeze() {
    local cur opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    opts="-g -h -v -o --version --help --gitpod --output"
    if [[ ${cur} == -* ]] ; then
        mapfile -t COMPREPLY < <(compgen -W "${opts}" -- "${cur}")
        return 0
    elif [[ ${cur} == r* ]]; then
        mapfile -t COMPREPLY < <(compgen -W "${opts}" "${cur}")
        return 0
    fi
}
complete -F _dockerfreeze dockerfreeze
